<?xml version="1.0"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <sitemap>
        <loc>{{ absolute_url(path('products-category:sitemap.urls')) }}</loc>
    </sitemap>

    {% for key, cat in category %}
        <sitemap>
            <loc>{{ absolute_url(path('products-product:sitemap', { category : key })) }}</loc>
        </sitemap>
    {% endfor %}
</sitemapindex>